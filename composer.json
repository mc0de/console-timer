{
    "name": "mc0de/console-timer",
    "description": "A trait for tracking execution time in Laravel console commands",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mc0de\\ConsoleTimer\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "symfony/console": "^6.0|^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mc0de\\ConsoleTimer\\TimerServiceProvider"
            ]
        }
    }
}
